<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ns.controller.OptionsController">
    <Button mnemonicParsing="false" onAction="#onBackButtonClick" text="Back">
        <cursor>
          <Cursor fx:constant="HAND" />
        </cursor>
      <VBox.margin>
         <Insets />
      </VBox.margin>
    </Button>
    <GridPane alignment="CENTER" layoutY="40.0" prefHeight="91.0" prefWidth="200.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <Label text="Theme" />
        <ChoiceBox fx:id="choicebox" prefWidth="150.0" GridPane.columnIndex="1">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ChoiceBox>
      <padding>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </padding>
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin>
    </GridPane>
   <padding>
      <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
   </padding>
</VBox>
